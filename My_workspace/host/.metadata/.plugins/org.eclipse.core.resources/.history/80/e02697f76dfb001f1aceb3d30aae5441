#include <stdio.h>

int main(void) {
	float num1, num2, num3;
	float average;

	printf("Select number 1: ");
	scanf("%f\n", &num1);

	printf("Select number 2: ");
	scanf("%f\n", &num2);

	printf("Select number 3: ");
	scanf("%f\n", &num3);

	average = (num1 + num2 + num3) / 3;
	printf("Your Average is: %f", average);
}
