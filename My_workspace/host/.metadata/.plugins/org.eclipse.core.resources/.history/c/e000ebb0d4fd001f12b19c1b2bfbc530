#include <stdio.h>

int main(void) {
	char c1 = 100;

	printf("Address of c1: %p\n", (char*) &c1);

	char* pointerC1 = &c1;


	char data = *pointerC1;
	printf("Data = %c\n", data);

	*pointerC1 = 65;

	printf("New Data = %c\n", data);

	printf("End Program")
}
