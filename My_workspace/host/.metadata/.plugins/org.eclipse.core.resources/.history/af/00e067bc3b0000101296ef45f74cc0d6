#include <stdio.h>
#include <stdint.h>

int main(void) {

	int num1, num2;

	printf("Enter first number (integer): ");
	scanf("%d", &num1);

	printf("\nEnter second number (integer): ");
	scanf("%d", &num2);

	if (num1 == num2) {
		printf("The numbers are equal.\n");
	}
	else {
		if (num1 < num2) {
			printf("%d (num2) is bigger\n", num2);
		}
		else {
			printf("%d (num1) is bigger\n", num2);
		}
	}

	printf("\nPress Enter Key to Exit the Application...");
	while(getchar() != '\n') {

	}
	getchar();

	return 0;
}
