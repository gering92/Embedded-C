#include <stdint.h>
#include <stdio.h>

void function_add_numbers(int a, int b, int c);

int main(void)
{
	function_add_numbers(2, 5, 3);
	return 0;
}


void function_add_numbers(int a, int b, int c) {
	int sum = a + b + c;

	printf("SUM = %d\n", sum);
}
